{
  "name": "@dvcol/import-map-plugin",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.14.3",
  "description": "Vite and rollup plugin to inject import map into index.html",
  "homepage": "https://github.com/dvcol/vite-plugin-import-map",
  "bugs": "https://github.com/dvcol/vite-plugin-import-map/issues",
  "keywords": [
    "typescript",
    "library",
    "vite",
    "wite-plugin",
    "rollup",
    "rollup-plugin",
    "importmap",
    "import-map",
    "html",
    "index.html",
    "esm",
    "micro-frontend"
  ],
  "license": "MIT",
  "author": "dvcol",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvcol/vite-plugin-import-map"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">= 7"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.cts"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "standard-version",
    "release:changelog": "extract-changelog-release > RELEASE.md"
  },
  "dependencies": {
    "@web/rollup-plugin-html": "^1.11.0",
    "chalk": "^4.1.2",
    "globby": "^11.1.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "vite": "^5.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@dvcol/eslint-plugin-presets": "^1.3.10",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^4.2.1",
    "extract-changelog-release": "^1.0.2",
    "jsdom": "^24.0.0",
    "standard-version": "^9.5.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
